{
    "parser": "babel-eslint",
    "rules": {
        "indent": [
            2,
            4
        ],
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
        "strict": 0,
        "curly": 0
    },
    "env": {
        "es6": true,
        "node": true
    }
}