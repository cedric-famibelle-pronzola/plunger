{
  "name": "plunger",
  "version": "0.2.0",
  "description": "Powerful link-to-dataset inspector",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/inspireteam/plunger"
  },
  "author": "Jérôme Desboeufs <jerome.desboeufs@data.gouv.fr>",
  "license": "MIT",
  "scripts": {
    "lint": "xo",
    "test": "nyc ava"
  },
  "bin": {
    "plunge": "bin/cli.js"
  },
  "dependencies": {
    "chalk": "^2.1.0",
    "commander": "^2.11.0",
    "content-disposition": "^0.5.2",
    "content-type": "^1.0.2",
    "debug": "^3.0.1",
    "execa": "^0.8.0",
    "feedparser": "^2.2.1",
    "file-type": "^6.1.0",
    "get-stream": "^3.0.0",
    "hasha": "^3.0.0",
    "htmlparser2": "^3.9.2",
    "lodash": "^4.17.4",
    "mime-types": "^2.1.16",
    "mississippi": "^1.3.0",
    "rfc2047": "^2.0.1",
    "rimraf": "^2.6.1",
    "type-is": "^1.6.15",
    "update-notifier": "^2.2.0"
  },
  "devDependencies": {
    "ava": "^0.22.0",
    "codecov": "^2.3.0",
    "nyc": "^11.2.1",
    "xo": "^0.19.0"
  },
  "engines": {
    "node": ">=8.x"
  },
  "ava": {
    "files": [
      "lib/**/*.test.js"
    ]
  },
  "nyc": {
    "all": true,
    "reporter": [
      "lcov",
      "text-summary"
    ]
  },
  "xo": {
    "semicolon": false,
    "space": 2,
    "rules": {
      "curly": "off",
      "capitalized-comments": "off"
    }
  }
}
